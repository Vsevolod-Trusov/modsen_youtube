{
  "compilerOptions": {
    "outDir": "./dist/",
    "allowJs": true,
    "moduleResolution": "node",
    "baseUrl": "./src",
    "paths": {
      "@/*": ["*"]
    },
    "esModuleInterop": true,
    "noImplicitAny": true,
    "module": "commonjs",
    "target": "es5",
    "strict": true,
    "types": ["cypress"],
    "jsx": "react"
  },
  "include": ["./src/**/*", "declarations.d.ts"]
}
